{
    "manifest_version": 3,
    "name": "khn",
    "version": "0.1.0",
    "description": "",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://nhk.or.jp/*",
                "*://*.nhk.or.jp/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "options_page": "options.html"
}